<script type="text/javascript">
(function($) {
$('.instagram-slick-carousel').slick({
  autoplay: false,
  dots: false,
  infinite: false,
  speed: 300,
  slidesToShow: 7,
  slidesToScroll: 6,
  variableWidth: true,
  responsive: [
    {
      breakpoint: 1024,
      settings: {
        slidesToShow: 5,
        slidesToScroll: 5,
        infinite: true,
        dots: false
      }
    },
    {
      breakpoint: 960,
      settings: {
        slidesToShow: 4,
        slidesToScroll: 4,
        infinite: true,
        dots: false
      }
    },
    {
      breakpoint: 600,
      settings: {
        slidesToShow: 3,
        slidesToScroll: 3
      }
    },
    {
      breakpoint: 480,
      settings: {
        slidesToShow: 2,
        slidesToScroll: 2
      }
    }
  ]
});
var instagram_slide_flag = true;
// On before slide change
$('.instagram-slick-carousel').on('beforeChange', function(event, slick, currentSlide, nextSlide){
  if(parseInt(slick.slideCount)-12 <= nextSlide) {
  	if(instagram_slide_flag == true) {
  		instagram_slide_flag = false;
  		var until_timestamp = $(this).data("until-timestamp");
  		var $this_slick_carousel = $(this);
  		jQuery.getJSON("https://q.crowdynews.com/v1/content/StraitTimes?q=stfoodtrending1476351675&sort=time&count=20&service=all&until="+until_timestamp,function( data ) {
  			$.each( data, function( key, instagram_photo ) {
  			    var item_html = '<div><div class="instagram-slide-item"> <a class="instagram-'+instagram_photo.type+'" target="_blank" href="'+instagram_photo.url+'"><div class="slider-img" style="background-image: url('+instagram_photo.media[0].url+');" ></div></a>'
            item_html += '</div><div class="author"> <a href="'+instagram_photo.author.url+'" target="_blank"><img class="photo-avatar" src="'+instagram_photo.author.image+'"></a> <div class="author-info"><a href="'+instagram_photo.author.url+'" target="_blank" title="@'+instagram_photo.author.username+'">@'+instagram_photo.author.username+'</a> </div></div>';
            item_html += '</div>';
  			    $this_slick_carousel.slick('slickAdd',item_html);
  			    var until_ts = Date.parse(instagram_photo.date)/1000;
  			    $this_slick_carousel.data("until-timestamp",until_ts);
  			  });
  			instagram_slide_flag = true;
  		});
  	}
  }
});

})(jQuery);
</script>       </div>
