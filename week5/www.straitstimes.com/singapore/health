(function($) {
if($('#food-search').length >= 1){
$('#food-search').keypress(function(e) {
    if (e.which === 13) {
        if ($('#food-search').value != "") {
          var food = $('#food-search').val();
            window.location = "http://www.straitstimes.com/search?searchkey="+ food + "&groups=/lifestyle/food/";
        }
        }
    });
  }
