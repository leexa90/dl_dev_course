    <div><div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 classy-panel-styles region"><div class="region-inner">
    <div class="panel-pane pane-fieldable-panels-pane pane-current-3189 pane-bundle-full-width-containers fullwidth-banner advanced-banner mask-left " id="newsletter-banner" >
      <div class="pane-content">
        
          <div class="nl-main">
<div class="nl">
    <div class="sub-1">
        <div class="sub-1-1">
            <div class="logo-1"><img src="/sites/all/themes/custom/bootdemo/masthead/min-300px-white.svg"></div>
            <div class="sub-text">Get ST newsletters in your inbox. <br/>Sign up here.</div>
            <div class="Field-1">
                  <div class="result">Subscribing...</div>
                  <div class="dropdown">
                    <button class="btn btn-default dropdown-toggle" type="button" id="menu1" data-toggle="dropdown">Select Newsletter
                    <span class="caret"></span></button>
                    <ul class="dropdown-menu" role="menu" aria-labelledby="menu1">
                        <li role="presentation">
                          <div class="checkbox">
                            <input type="checkbox" value='11077' name="edition" id="checkbox_11077">
                            <label for="checkbox_11077"><span></span> ST Morning Briefing</label>
                          </div>
                        </li>
                        <li role="presentation">
                          <div class="checkbox">
